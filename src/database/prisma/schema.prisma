// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["public", "raw_data"]
}

enum Maille {
  NAT  @map("nat")
  DEPT @map("dept")
  REG  @map("reg")

  @@map("maille")
  @@schema("public")
}

model territoire {
  code        String       @id
  nom         String
  nom_affiche String
  maille      Maille
  code_insee  String
  trace_svg   String
  code_parent String?
  chantier    chantier[]
  indicateur  indicateur[]
  zone_id     String

  @@schema("public")
}

enum type_ate {
  ate
  hors_ate_deconcentre
  hors_ate_centralise

  @@schema("public")
}

enum type_statut {
  BROUILLON
  PUBLIE
  NON_PUBLIE

  @@schema("public")
}

model chantier {
  id                                 String
  nom                                String
  perimetre_ids                      String[]
  taux_avancement                    Float?
  taux_avancement_precedent          Float?
  territoire_nom                     String?
  code_insee                         String
  maille                             String
  directeurs_administration_centrale String[]
  directeurs_projet                  String[]
  ministeres                         String[]
  directions_administration_centrale String[]
  meteo                              String?    @default("NON_RENSEIGNEE")
  axe                                String     @default("non renseignée")
  ppg                                String     @default("non renseignée")
  directeurs_projet_mails            String[]
  est_barometre                      Boolean?
  est_territorialise                 Boolean?
  territoire                         territoire @relation(fields: [territoire_code], references: [code], onDelete: Cascade, onUpdate: Cascade)
  territoire_code                    String
  ate                                type_ate?
  a_taux_avancement_departemental    Boolean?
  a_taux_avancement_regional         Boolean?
  a_meteo_departemental              Boolean?
  a_meteo_regional                   Boolean?
  est_applicable                     Boolean?
  statut                             type_statut @default(PUBLIE)
  a_supprimer                        Boolean     @default(true)

  @@id([id, code_insee, maille])
  @@schema("public")
}

model ministere {
  id          String      @id
  nom         String
  perimetre   perimetre[]
  icone       String?
  a_supprimer Boolean     @default(true)

  @@schema("public")
}

model perimetre {
  id              String     @id
  nom             String
  ministere       String?
  ministere_table ministere? @relation(fields: [ministere_id], references: [id], onDelete: Cascade, onUpdate: Cascade)
  ministere_id    String?
  a_supprimer     Boolean    @default(true)

  @@schema("public")
}

model indicateur {
  id                                       String
  nom                                      String
  chantier_id                              String
  objectif_valeur_cible                    Float?
  objectif_taux_avancement                 Float?
  objectif_date_valeur_cible               DateTime?  @db.Date
  type_id                                  String?
  type_nom                                 String?
  est_barometre                            Boolean?
  est_phare                                Boolean?
  valeur_initiale                          Float?
  date_valeur_initiale                     DateTime?  @db.Date
  valeur_actuelle                          Float?
  date_valeur_actuelle                     DateTime?  @db.Date
  territoire_nom                           String?
  code_insee                               String
  maille                                   String
  evolution_valeur_actuelle                Float[]
  evolution_date_valeur_actuelle           DateTime[] @db.Date
  description                              String?
  source                                   String?
  mode_de_calcul                           String?
  unite_mesure                             String?
  territoire                               territoire @relation(fields: [territoire_code], references: [code], onDelete: Cascade, onUpdate: Cascade)
  territoire_code                          String
  ponderation_dept                         Float?
  ponderation_reg                          Float?
  ponderation_nat                          Float?
  objectif_valeur_cible_intermediaire      Float?
  objectif_taux_avancement_intermediaire   Float?
  objectif_date_valeur_cible_intermediaire DateTime?  @db.Date
  est_applicable                           Boolean?
  a_supprimer                              Boolean    @default(true)
  dernier_import_date                      DateTime?  @db.Date
  dernier_import_rapport_id                String?    @db.Uuid
  dernier_import_auteur                    String?
  dernier_import_date_indic                DateTime?  @db.Date
  dernier_import_rapport_id_indic          String?    @db.Uuid
  dernier_import_auteur_indic              String?

  @@id([id, code_insee, maille])
  @@schema("public")
}

model synthese_des_resultats {
  id               String    @id
  chantier_id      String
  maille           String
  code_insee       String
  auteur           String?
  meteo            String?
  date_meteo       DateTime? @db.Timestamp
  commentaire      String?
  date_commentaire DateTime? @db.Timestamp

  @@schema("public")
}

model axe {
  id          String  @id
  nom         String
  a_supprimer Boolean @default(true)

  @@schema("public")
}

model ppg {
  id          String  @id
  nom         String
  a_supprimer Boolean @default(true)

  @@schema("public")
}

model commentaire {
  id          String   @id
  chantier_id String
  type        String
  contenu     String
  date        DateTime @db.Timestamp
  auteur      String
  maille      String
  code_insee  String

  @@schema("public")
}

model mesure_indicateur {
  id           String                           @id @db.Uuid
  indic_id     String
  zone_id      String
  metric_date  String
  metric_type  String
  metric_value String
  rapport_id   String                           @db.Uuid
  rapport      rapport_import_mesure_indicateur @relation(fields: [rapport_id], references: [id])
  date_import  DateTime?                        @default(now()) @db.Timestamptz

  @@schema("raw_data")
}

model metadata_indicateurs {
	indic_id              String @id
	indic_parent_indic    String?
	indic_parent_ch       String
	indic_nom             String
	indic_nom_baro        String?
	indic_descr           String?
	indic_descr_baro      String?
	indic_is_perseverant  Boolean?
	indic_is_phare        Boolean?
	indic_is_baro         Boolean?
	indic_type            String?
	indic_source          String?
	indic_source_url      String?
	indic_methode_calcul  String?
	indic_unite           String?
	indic_hidden_pilote   Boolean?
	indic_schema          String?
	zg_applicable         String?

  @@schema("raw_data")
}

model metadata_parametrage_indicateurs {
	indic_id                  String @id
	vi_dept_from              String?
	vi_dept_op                String?
	va_dept_from              String?
	va_dept_op                String?
	vc_dept_from              String?
	vc_dept_op                String?
	vi_reg_from               String?
	vi_reg_op                 String?
	va_reg_from               String?
	va_reg_op                 String?
	vc_reg_from               String?
	vc_reg_op                 String?
	vi_nat_from               String?
	vi_nat_op                 String?
	va_nat_from               String?
	va_nat_op                 String?
	vc_nat_from               String?
	vc_nat_op                 String?
	param_vaca_decumul_from   String?
	param_vaca_partition_date String?
	param_vaca_op             String?
	param_vacg_decumul_from   String?
	param_vacg_partition_date String?
	param_vacg_op             String?
	poids_pourcent_dept       Int?
	poids_pourcent_reg        Int?
	poids_pourcent_nat        Int?
	tendance                  String?

  @@schema("raw_data")
}

model metadata_indicateurs_complementaire {
	indic_id                  String @id
	reforme_prioritaire       String?
	projet_annuel_perf        Boolean?
	detail_projet_annuel_perf String?
	periodicite               String?
	delai_disponibilite       Int?
	indic_territorialise      Boolean?
	frequence_territoriale    String?
	mailles                   String?
	admin_source              String?
	methode_collecte          String?
	si_source                 String?
	donnee_ouverte            Boolean?
	modalites_donnee_ouverte  String?
	resp_donnees              String?
	resp_donnees_email        String?
	contact_technique         String?
	contact_technique_email   String?
	commentaire               String?

  @@schema("raw_data")
}

model mesure_indicateur_temporaire {
  id           String                           @id @db.Uuid
  indic_id     String?
  zone_id      String?
  metric_date  String?
  metric_type  String?
  metric_value String?
  rapport_id   String                           @db.Uuid
  rapport      rapport_import_mesure_indicateur @relation(fields: [rapport_id], references: [id])
  date_import  DateTime?                        @default(now()) @db.Timestamptz

  @@schema("public")
}

model erreur_validation_fichier {
  id                String                           @id @db.Uuid
  nom               String
  cellule           String
  message           String
  nom_du_champ      String
  numero_de_ligne   Int
  position_de_ligne Int
  position_du_champ Int
  rapport_id        String                           @db.Uuid
  rapport           rapport_import_mesure_indicateur @relation(fields: [rapport_id], references: [id])

  @@schema("public")
}

model rapport_import_mesure_indicateur {
  id                           String                         @id @db.Uuid
  date_creation                DateTime                       @db.Timestamptz
  utilisateurEmail             String                         @map("utilisateur_email")
  est_valide                   Boolean                        @default(false)
  utilisateur                  utilisateur                    @relation(fields: [utilisateurEmail], references: [email])
  mesure_indicateur_temporaire mesure_indicateur_temporaire[]
  erreur_validation_fichier    erreur_validation_fichier[]
  mesure_indicateur            mesure_indicateur[]

  @@schema("public")
}

enum type_objectif {
  notre_ambition
  deja_fait
  a_faire

  @@schema("public")
}

model objectif {
  id          String        @id
  auteur      String
  type        type_objectif
  contenu     String
  date        DateTime      @db.Timestamp
  chantier_id String

  @@schema("public")
}

enum type_decision_strategique {
  suivi_des_decisions

  @@schema("public")
}

model decision_strategique {
  id          String                    @id
  auteur      String
  type        type_decision_strategique
  contenu     String
  date        DateTime                  @db.Timestamp
  chantier_id String

  @@schema("public")
}

model projet_structurant {
  id                       String    @id
  code                     String
  nom                      String
  territoire_code          String
  perimetres_ids           String[]
  direction_administration String[]
  chefferie_de_projet      String[]
  co_porteurs              String[]
  taux_avancement          Float?
  date_taux_avancement     DateTime? @db.Timestamp
  date_donnees_qualitative DateTime? @db.Timestamp
  a_supprimer              Boolean   @default(true)

  @@schema("public")
}

model indicateur_projet_structurant {
  id                      String    @id
  nom                     String
  projet_structurant_code String
  type_id                 String?
  description             String?
  source                  String?
  mode_de_calcul          String?
  valeur_actuelle         Float?
  valeur_initiale         Float?
  valeur_cible            Float?
  taux_avancement         Float?
  date_valeur_actuelle    DateTime? @db.Timestamp
  date_valeur_initiale    DateTime? @db.Timestamp
  date_valeur_cible       DateTime? @db.Timestamp
  date_taux_avancement    DateTime? @db.Timestamp
  projet_structurant_id   String
  a_supprimer             Boolean   @default(true)

  @@schema("public")
}

enum type_objectif_projet_structurant {
  suivi_des_objectifs

  @@schema("public")
}

model objectif_projet_structurant {
  id                    String                           @id
  projet_structurant_id String
  type                  type_objectif_projet_structurant
  contenu               String
  date                  DateTime                         @db.Timestamp
  auteur                String?

  @@schema("public")
}

model synthese_des_resultats_projet_structurant {
  id                    String    @id
  projet_structurant_id String
  meteo                 String?
  date_meteo            DateTime? @db.Timestamp
  commentaire           String?
  date_commentaire      DateTime? @db.Timestamp
  auteur                String?

  @@schema("public")
}

model commentaire_projet_structurant {
  id                    String   @id @db.Uuid
  projet_structurant_id String
  type                  String
  contenu               String
  date                  DateTime @db.Timestamp
  auteur                String?

  @@schema("public")
}

model perimetre_projet_structurant {
  id                 String  @id
  perimetres_ppg_nom String
  perimetres_ppg_id  String
  perimetre_ps_nom   String
  a_supprimer        Boolean @default(true)

  @@schema("public")
}

model utilisateur {
  id                               String                             @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email                            String                             @unique
  nom                              String
  prenom                           String
  fonction                         String?
  profil                           profil                             @relation(fields: [profilCode], references: [code], onDelete: Cascade, onUpdate: Cascade)
  profilCode                       String                             @map("profil_code")
  habilitation                     habilitation[]
  rapport_import_mesure_indicateur rapport_import_mesure_indicateur[]
  date_modification                DateTime                           @default(now())
  auteur_modification              String

  @@schema("public")
}

model profil {
  code                                                        String        @id
  nom                                                         String
  a_acces_tous_chantiers                                      Boolean       @default(false)
  a_acces_tous_chantiers_territorialises                      Boolean       @default(false)
  projets_structurants_lecture_tous_perimetres                Boolean       @default(false)
  projets_structurants_lecture_meme_perimetres_que_chantiers  Boolean       @default(false)
  projets_structurants_lecture_tous_territoires               Boolean       @default(false)
  projets_structurants_lecture_meme_territoires_que_chantiers Boolean       @default(false)
  a_acces_tous_les_territoires_lecture                        Boolean       @default(false)
  a_acces_tous_les_territoires_saisie_commentaire             Boolean       @default(false)
  a_acces_tous_les_territoires_saisie_indicateur              Boolean       @default(false)
  peut_consulter_les_utilisateurs                             Boolean       @default(false)
  peut_modifier_les_utilisateurs                              Boolean       @default(false)
  peut_supprimer_les_utilisateurs                             Boolean       @default(false)
  peut_saisir_des_commentaires                                Boolean       @default(false)
  a_access_aux_chantiers_brouillons                           Boolean       @default(false)
  utilisateur                                                 utilisateur[]

  @@schema("public")
}

model habilitation {
  utilisateur   utilisateur @relation(fields: [utilisateurId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  utilisateurId String      @map("utilisateur_id") @db.Uuid
  scope         scope       @relation(fields: [scopeCode], references: [code], onDelete: Cascade, onUpdate: Cascade)
  scopeCode     String      @map("scope_code")
  territoires   String[]
  perimetres    String[]
  chantiers     String[]

  @@unique([utilisateurId, scopeCode])
  @@schema("public")
}

model scope {
  code         String         @id
  nom          String
  habilitation habilitation[]

  @@schema("public")
}

model historisation_modification {
  id                   String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  id_objet_modifie     String
  utilisateur_nom      String
  type_de_modification String
  date_de_modification String
  table_modifie_id     String
  ancienne_valeur      Json?
  nouvelle_valeur      Json?

  @@schema("public")
}
