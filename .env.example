################################################################################
#                                 WebApp .env                                  #
################################################################################

##
# Database connection string for Prisma

DATABASE_URL="postgresql://postgresql:secret@localhost:5432/postgresql"

##
# Optional auth to activate NextAuth basic auth (soon to be replaced by OIDC)

BASIC_AUTH_CREDENTIALS=user:password

##
# Keycloak and OAuth

# NextAuth secret can be generated by using `$ openssl rand -base64 32`
NEXTAUTH_SECRET=TBD

KEYCLOAK_ID=TBD
KEYCLOAK_SECRET=TDB
KEYCLOAK_ISSUER=TDB
